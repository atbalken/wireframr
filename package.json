{
  "name": "wireframr",
  "description": "Sketch Plugin to change your high-fidelity screendesign to simple wireframes.",
  "repository": "https://github.com/chewbecky/wireframr.git",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "wireframr",
    "manifest": "src/manifest.json",
    "main": "wireframr.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0",
    "prettier": "^2.0.5"
  },
  "author": "chewbecca <beckybettinger@yahoo.de>"
}
